<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="it.dto">
<class name="Accountant" table="Accountanttable">
<id name="accountantContactNo" column="accountantContactNo"/>
<property name="accountantInchargeName" column="AccountantName"/>
<property name="accountantEmail" column="accountantEmail"/>
<property name="month"/>
<property name="monthlyProfit" column="monthlyProfit"/>
<property name="monthlyMaintenanceCost" column="monthlyMaintenanceCost"/>
<property name="statement" column="statement"/>
</class> 

</hibernate-mapping>
